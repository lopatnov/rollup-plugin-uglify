{
  "name": "@lopatnov/rollup-plugin-uglify",
  "version": "3.0.0",
  "description": "A Rollup plugin for minifying JavaScript bundles using Terser",
  "author": {
    "name": "Oleksandr Lopatnov",
    "url": "https://github.com/lopatnov"
  },
  "license": "Apache-2.0",
  "homepage": "https://lopatnov.github.io/rollup-plugin-uglify/",
  "umdName": "uglify",
  "source": "src/plugin.ts",
  "main": "dist/plugin.cjs.js",
  "module": "dist/plugin.es.js",
  "browser": "dist/plugin.umd.js",
  "types": "dist/plugin.d.ts",
  "exports": {
    ".": {
      "import": "./dist/plugin.es.js",
      "require": "./dist/plugin.cjs.js",
      "browser": "./dist/plugin.umd.js",
      "types": "./dist/plugin.d.ts"
    }
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "uglify",
    "minify",
    "minifier",
    "terser",
    "javascript",
    "compression",
    "optimization",
    "bundle",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lopatnov/rollup-plugin-uglify.git"
  },
  "bugs": {
    "url": "https://github.com/lopatnov/rollup-plugin-uglify/issues"
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -cw",
    "test": "jest",
    "prepare": "npm i --no-save --no-package-lock terser@latest",
    "configure-npm": "node ./set-registry.js -s https://registry.npmjs.org",
    "configure-gpr": "node ./set-registry.js -s https://npm.pkg.github.com",
    "update": "npx npm-check-updates -u && npm install"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@rollup/pluginutils": "^5.3.0",
    "core-js": "^3.47.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "jest-config": "^30.2.0",
    "rollup": "^4.55.1",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "rollup": ">=4.0.0",
    "terser": ">=4.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
